!ent %_start @ 0x1000

@hello align0 resb 0
@hello_elem0 align1 "Hello Everyone!"
%_start
    mov i64 rg @hello
    mov i64 rh @hello_elem0
    call i64 %printstrln_asm
    hlt

%printstrln_asm
    push i64 rb
    mov i64 rb rg
%printstrln_asm_loop
    cmp i8 [rb] r0
    jeq i64 %printstrln_asm_ret
    printc i8 [rb]
    add i64 rb r1
    jmp i64 %printstrln_asm_loop
%printstrln_asm_ret
    printc i8 $0xa ; newline
    pop i64 rb
    ret

